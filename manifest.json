{
	"sap.app": {
		"id": "sap.viz.sample.Pie",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.viz.sample.Pie.i18n.i18n"
				}
			}
		},
		"rootView": {
			"viewName": "sap.viz.sample.Pie.Pie",
			"type": "XML",
			"async": true
		},
		"resources": {
			"css": [{
				"uri": "/exploredStyle.css"
			}]
		},
		"dependencies": {
			"libs": {
				"sap.viz": {},
				"sap.ui.layout": {},
				"sap.m": {}
			}
		},
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"Pie.view.xml",
					"Pie.controller.js",
					"InitPage.js",
					"manifest.json"
				]
			}
		}
	}
}